<!DOCTYPE html>
<html>
<head>
  <title>Edit Result</title>
</head>
<body>
  <h1>Edit Result</h1>
  <!-- Edit result form -->
  <form action="/teacher/edit/<%= results.rollNumber %>" method="POST">
    <!-- Roll number input -->
    <label for="rollNumber">Roll Number:</label>
    <input type="text" id="rollNumber" name="rollNumber" value="<%= results.rollNumber %>" required><br>

    <!-- Date of birth input -->
    <label for="dob">Date of Birth:</label>
    <input type="date" id="dob" name="dob" value="<%= results.dob %>" required><br>

    <!-- Name input -->
    <label for="name">Name:</label>
    <input type="text" id="name" name="name" value="<%= results.name %>" required><br>

    <!-- Total score input -->
    <label for="totalScore">Total Score:</label>
    <input type="number" id="totalScore" name="totalScore" value="<%= results.totalScore %>" required><br>

    <button type="submit">Update Result</button>
  </form>
</body>
</html>
